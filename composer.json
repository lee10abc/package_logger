{
    "name": "jean/logger",
    "description": "Package for Laravel Logger",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Jean\\Logger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "lee10abc",
            "email": "lee10abc@naver.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jean\\Logger\\LoggerServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "squizlabs/php_codesniffer": "^3.11"
    },
    "scripts": {
        "pint": "./vendor/bin/pint -v",
        "sniffer": "./vendor/bin/phpcbf"
    }
}
